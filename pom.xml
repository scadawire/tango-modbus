   <name>Modbus</name>
   <description>Modbus device</description>
   <build>
       <plugins>
           <plugin>
               <groupId>org.freehep</groupId>
               <artifactId>freehep-nar-plugin</artifactId>
               <configuration>
                   <cpp>
                       <defines>
